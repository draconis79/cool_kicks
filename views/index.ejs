
<!DOCTYPE html>
<html>
  <head>
    <% include partials/head.ejs %>

  </head>
  <body>
    <% include partials/nav.ejs %>
    <div class="divHeader"><h2>Sneakers</h2> </div>

    <a href="/sneakers/new"><div class="addbtn">Add a Sneaker!</div></a>

    <%# for loop%>
    <div class="container">

    <% for (let i = 0; i < allSneakers.length; i++) { %>
  <div class="item">
      <a href="/sneakers/<%= allSneakers[i].id %>"><h2> <%= allSneakers[i].name %> </h2></a>


            <div class='cheeseImg'><a href="/sneakers/<%= allSneakers[i].id %>"><img src= <%= allSneakers[i].image %> alt="image goes here"></a></div>

             <br><br>

      </div>
    <% } %>


    </div>


  </body>
</html>
